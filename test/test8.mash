import PrintLn "crt.lib" "PRINTLN"
import InputLn "crt.lib" "INPUTLN"

class MyClassParent:
  public:
    var a

    init(a):
      println("Hello from parent!")
      $a ?= a
    end
end

class MyClass(MyClassParent):
  public:
    var b

    init(a, b):
      super(a)
      $b ?= b
    end

    proc out(xxx):
      println(xxx)
    end
end

func MyClass::Summ():
  return $a + $b
end

x ?= new MyClass(2, 3)
println(x -> summ())

try:
  1 / 0
catch E:
  println(E -> Message)
end

func buzz(k):
  if k > 1:
    safe j ?= buzz(k - 1)
    println(j)
  end
  return k
end

buzz(10)

enum MyEnum[
  meHello="Hello"
]

MyClass -> Out(meHello)
