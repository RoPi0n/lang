import PrintLn "crt.lib" "PRINTLN"

func Out(a):
  "Next number is a " + (a + 1)
end

a ?= 0
while ~(a >= 10):
  PrintLn(Out(a))
  a++
end



func len(a):
  inline "push len.a"
  inline "alen"
end

s ?= "Hello world!"
i ?= 0
while i < len(s):
  println(s[i])
  i++
end

