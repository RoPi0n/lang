uses <bf>
uses <crt>
uses <coroutines>

func class::MyCoroutineFunc(line):
  c ?= 0

  while c < len(line):
    $Yield(line[c])
    c++
  end

  return null
end

proc main():
  F ?= new Coroutine(class::MyCoroutineFunc)

  r ?= F -> Call("Hello world!")
  while NotNull(r):
    print(r)
    sleep(100)
    r ?= F -> Call()
  end

  F -> Free()
  Crt.ReadKey()
end
