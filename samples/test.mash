uses <classes\list>

class MyClass:
  var a, b
  proc Create
end

proc MyClass::Create():
  $a ?= new list()
  $b ?= new list()
  $a -> add(new MyClass)
  $b -> add(new MyClass)
end

proc main():
  while true:
    x ?= new list()

    for i in 1..100:
      x -> add(new MyClass())
    end

    gc()
  end
end
