uses <crt>
uses <math\matrix>

proc main():
  // Test 1
  a ?= [ [4, 2],
         [3, 1],
         [1, 5] ]

  b ?= [ [1, 2, 2],
         [3, 1, 1] ]

  println( matrix.Multiply(a, b) )

  // Test 2
  a ?= [ [1, 2, 2],
         [3, 1, 1] ]

  b ?= [ [4, 2],
         [3, 1],
         [1, 5] ]

  println( matrix.Multiply(a, b) )

  // Test 3
  a ?= [ [ 3, 1],
         [ 2, 7] ]

  println( matrix.det(a) )  // Should be 19

  // Test 4
  a ?= [ [ 1, -2,  3],
         [ 4,  0,  6],
         [-7,  8,  9] ]

  println( matrix.det(a) )  // Should be 204

  // Test 5
  a ?= [ [ 7,  1,  3],
         [ 2,  4,  1],
         [ 1,  5,  1] ]

  println( matrix.det(a) )  // Should be 10


  // Test 6
  a ?= [ [ 3, -3, -5,  8],
         [-3,  2,  4, -6],
         [ 2, -5, -7,  5],
         [-4,  3,  5, -6] ]

  println( matrix.det(a) )  // should be 18

  // Test 7

  a ?= [ [1,  0,  2, -1],
         [3,  0,  0,  5],
         [2,  1,  4, -3],
         [1,  0,  5,  0] ]

  println( matrix.det(a) )

  // Test 8

  a ?= [ [1,  0],
         [3,  0],
         [2,  1],
         [1,  0] ]

  println( matrix.transp(a) )

  inputln()
end
