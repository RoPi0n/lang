uses <crt>
uses <calls>

func f(a, b):
  return a + b
end

func f2(a, b):
  return a * b
end

class xclass:
  var a
  proc create
end

proc xclass::create(a):
  $a ?= a
end


proc main():
  while true:
    m ?= new Method(f)
    h ?= new Hook(@f, f2)

    m -> call(10, 20)
    f(10, 20)

    gc()
  end

  inputln()
end
